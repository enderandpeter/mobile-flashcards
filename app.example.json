{
  "expo": {
    "name": "Mobile Flashcards",
    "slug": "mobile-flashcards",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "spencer-williams",
            "project": "mobile-flashcards",
            "authToken": "authToken"
          }
        }
      ]
    },
    "description": "A very simple mobile app for managing flash cards and creating quizzes, born from Udacity's React Nanodegree final project.",
    "githubUrl": "https://github.com/enderandpeter/mobile-flashcards"
  }
}